<Window x:Class="Alchemy_Engine.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Alchemy_Engine"
        mc:Ignorable="d"
        Title="Alchemy-Engine" Height="450" Width="330" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image Height="130" Margin="10,48,10,0" VerticalAlignment="Top" Source="assets/logo.png" StretchDirection="DownOnly"/>
        <Button x:Name="btnPallete" Content="Palette Generator" Margin="10,236,10,145"/>
        <Button x:Name="btnCovert" Content="Convert Image" Margin="10,281,10,100"/>
        <Button x:Name="btnGenerator" Content="Solid Generator" Margin="10,326,10,55"/>
        <Button x:Name="btnLumetri" Content="Lumetri Editor" Margin="10,371,10,10"/>
    </Grid>
</Window>
